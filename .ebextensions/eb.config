container_commands:
  01migrate:
    command: docker exec `docker ps --no-trunc -q | head -n 1` rake bin/db:migrate --noinput | tee /tmp/db_migrate.log
    leader_only: true
  02assets:
    command: docker exec `docker ps --no-trunc -q | head -n 1` rake bin/assets:precompile --noinput | tee /tmp/assets_precompile.log

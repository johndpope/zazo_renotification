container_commands:
  01migrate:
    command: docker exec `docker ps --no-trunc -q | head -n 1` rake db:migrate --noinput | tee /tmp/db_migrate.log
    leader_only: true
  02assets:
    command: docker exec `docker ps --no-trunc -q | head -n 1` rake assets:precompile --noinput | tee /tmp/assets_precompile.log

{
  "AWSEBDockerrunVersion": 1,
  "Image": {
    "Name": "asux/rails-base",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "3000"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current/tmp",
      "ContainerDirectory": "/usr/src/app/tmp"
    }
  ],
  "Logging": "/usr/src/app/log"
}
